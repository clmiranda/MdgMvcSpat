@model User
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="container-fluid my-5">
    <h3>
        Vista actualizar email
    </h3>
    <p class="mb-4">
        En esta vista puedes actualizar el correo electronico vinculado a la cuenta.
    </p>
    <div id="body-content-action">
        @await Html.PartialAsync("PartialView/_ActualizarEmail", Model)
    </div>
</div>